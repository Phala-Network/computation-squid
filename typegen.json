{
  "$schema": "https://raw.githubusercontent.com/subsquid/squid-sdk/master/substrate/substrate-typegen/src/config.schema.json",
  "outDir": "src/types",
  "specVersions": "https://v2.archive.subsquid.io/metadata/khala",
  "pallets": {
    "PhalaStakePoolv2": {
      "events": [
        "PoolCreated",
        "PoolCommissionSet",
        "PoolCapacitySet",
        "PoolWorkerAdded",
        "PoolWorkerRemoved",
        "WorkingStarted",
        "RewardReceived",
        "OwnerRewardsWithdrawn",
        "Contribution",
        "WorkerReclaimed"
      ]
    },
    "PhalaVault": {
      "events": [
        "PoolCreated",
        "VaultCommissionSet",
        "OwnerSharesGained",
        "OwnerSharesClaimed",
        "Contribution"
      ]
    },
    "PhalaBasePool": {
      "events": [
        "NftCreated",
        "Withdrawal",
        "WithdrawalQueued",
        "PoolWhitelistCreated",
        "PoolWhitelistDeleted",
        "PoolWhitelistStakerAdded",
        "PoolWhitelistStakerRemoved"
      ]
    },
    "PhalaComputation": {
      "events": [
        "WorkerStarted",
        "WorkerStopped",
        "WorkerReclaimed",
        "SessionBound",
        "SessionUnbound",
        "WorkerEnterUnresponsive",
        "WorkerExitUnresponsive",
        "SessionSettled",
        "BenchmarkUpdated",
        "TokenomicParametersChanged"
      ],
      "storage": ["TokenomicParameters"]
    },
    "PhalaRegistry": {
      "events": ["WorkerAdded", "WorkerUpdated", "InitialScoreSet"]
    },
    "RmrkCore": {
      "events": ["CollectionCreated", "NftMinted", "PropertySet", "NFTBurned"]
    },
    "Identity": {
      "events": ["IdentitySet", "IdentityCleared", "JudgementGiven"],
      "storage": ["IdentityOf"]
    }
  }
}
